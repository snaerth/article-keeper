{
  "name": "starter-kit-universally",
  "version": "13.0.0",
  "description": "A starter kit for universal react applications.",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "analyze:client": "babel-node internal/scripts/analyze --client",
    "analyze:server": "babel-node internal/scripts/analyze --server",
    "build":
      "npm run clean && cross-env NODE_ENV=production babel-node internal/scripts/build --optimize",
    "build:dev":
      "npm run clean && cross-env NODE_ENV=development babel-node internal/scripts/build",
    "clean": "cross-env babel-node internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "dev": "cross-env DEPLOYMENT=development babel-node internal/development",
    "dev-remote":
      "cross-env DEPLOYMENT=development babel-node internal/scripts/ngrok",
    "eslint": "eslint client server shared config internal api",
    "fix-sass": "npm rebuild node-sass",
    "lint": "npm run eslint && npm run stylelint",
    "preinstall": "node internal/scripts/preinstall",
    "precommit": "npm run lint && npm run test",
    "start": "cross-env NODE_ENV=production node build/server",
    "stylelint": "stylelint \"shared/**/*.scss\" --syntax scss",
    "test": "jest",
    "test:watch": "jest -- --watch",
    "test:coverage": "jest -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "jest": {
    "collectCoverageFrom": [
      "shared/**/*.{js,jsx}",
      "server/**/*.{js}",
      "api/**/*.{js}"
    ],
    "snapshotSerializers": ["<rootDir>/node_modules/enzyme-to-json/serializer"],
    "moduleDirectories": ["node_modules", "shared"],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/internal/jest/styleMock.js",
      "\\.(je?pg|png|gif|ico|otf|ttf|eot|woff|woff2|svg)$":
        "<rootDir>/internal/jest/assetMock.js",
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|public|debug)/"
    ]
  },
  "dependencies": {
    "animejs": "2.2.0",
    "app-root-dir": "^1.0.2",
    "assets-webpack-plugin": "^3.5.1",
    "async": "^2.5.0",
